{
	"name": "Tmall MiaoSha",
	"version": "1.0",
    "manifest_version": 2,
    "description": "天猫秒杀插件",
    "browser_action": {
		"default_icon": "./images/icon.png",
		"default_title": "Tmall秒杀"
	},
	"permissions": [
		"tabs",
    	"https://*/*",
    	"http://*/*"
	],
  	"content_scripts": [
        {
            "matches": ["*://*.tmall.com/*id=544751208475*skuId=3447571490096*"],
            "js": ["jquery-1.11.3.min.js", "tmall.js"]
        }
    ]
}
